<style>*,:after,:before{-webkit-tap-highlight-color:transparent;box-sizing:border-box;margin:0}html{block-size:100%}body{align-content:center;display:grid;grid-row-gap:4px;justify-content:center;min-block-size:100%}</style></head><body><form id="gotourl"><input type="url" name="url" id="url" placeholder="Type a URL" pattern="https://.*" size="30" required><button type="submit">run</button></form><button id="newtab">open in new tab</button><script type="text/javascript">var getURL=function(a){var b=window.location.search.substring(1),g=function(e){var c=e.split("=");e=decodeURIComponent(c[0]);c=decodeURIComponent(c[1]);return e==a?c:null},d=null;if(-1<b.indexOf("&")){b=b.split("&");for(var f=0;f<b.length&&(d=g(b[f]),null===d);f++);}else d=g(b);return d},local="",input=document.getElementById("url"),form=document.getElementById("gotourl"),out=getURL("url"),newtab=document.getElementById("newtab"),request=sessionStorage.getItem("url");input.addEventListener("input",function(a){sessionStorage.setItem("url",a.target.value);return local=a.target.value});form.addEventListener("submit",function(a){a.preventDefault();document.location.href=local;sessionStorage.removeItem("url")});out?document.location.href=out:newtab&&request&&newtab.addEventListener("click",function(a){window.open(request,"_blank");sessionStorage.removeItem("url")})</script></body></html>
